
Var vao: Natural = genvertexarray()
bindvertexarray vao

Execute "create progs"
Execute "get locations"
Execute "init models"
Execute "init textures"


// ------------------
// other
// ------------------

clearcolor vec(.1,.4,.2,1)
enable depth_test
enable  cull_face
frontface cw
cullface back

Shared Var bias: Matrix = 0.5 * translation(vec(1, 1, 1, 1))